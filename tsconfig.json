{
  "compilerOptions": {
    "outDir": "./build/dist/",
    "noImplicitAny": true,
    "downlevelIteration": true,
    "module": "CommonJS",
    "target": "ES5",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "lib": ["ESNext", "DOM", "dom.iterable"],
    "types": ["jasmine"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
